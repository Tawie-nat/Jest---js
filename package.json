{
  "name": "jest",
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest": "^28.1.0"
  }
}
